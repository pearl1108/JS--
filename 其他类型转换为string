//方法一：调用被转换数据类型的toString（）方法
//不会影响原变量，会将转化的结果返回，需要一个变量来放置这个结果。
var a = 123;
			a = a.toString();
			console.log(a);
			console.log(typeof a);

 //null和undefined没有toString（）方法，若调用会报错
 var b = null;
			b = b.toString();
			console.log(b);
			console.log(typeof b);
      
      
      
 //方法二：调用String()函数，并将被转换的数据作为参数传递给函数
 var c = null;
			c = String(c);
			console.log(c);
			console.log(typeof c);
